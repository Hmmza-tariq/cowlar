{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Flutter Run",
			"type": "shell",
			"command": "flutter",
			"args": [
				"run",
				"--flavor",
				"cowlar_dev",
				"-t",
				"lib/main.dart"
			],
			"group": "build",
			"problemMatcher": [
				"$dart-analyzer"
			],
			"isBackground": false
		}
	]
}